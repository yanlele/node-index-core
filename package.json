{
  "name": "index-core",
  "version": "1.0.0",
  "description": "node-index 文章代码部分",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prettier": "prettier --write",
    "commit": "commit",
    "type-check": "tsc",
    "preview": "lerna run preview --stream --scope",
    "lint": "lerna run --parallel lint"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yanlele/node-index-core.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yanlele/node-index-core/issues"
  },
  "homepage": "https://github.com/yanlele/node-index-core#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "yarn prettier",
      "git add"
    ],
    "**/*.{ts,spec.js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/prompt-cli": "^7.5.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^11.12.2",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "glob": "^7.1.3",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lerna": "^3.13.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "puppeteer": "^2.0.0",
    "ts-jest": "^24.1.0",
    "tsconfig": "^5.0.3",
    "tslint": "^5.15.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.1"
  },
  "dependencies": {}
}
